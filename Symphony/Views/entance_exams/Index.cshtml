@model IEnumerable<Symphony.entance_exams>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_BackEndLayout.cshtml";
}

<h2>Entrance Exam Details Admin Portal</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.roll_num)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.s_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.marks)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.c_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fees)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.cell)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.user_id)
        </th>

        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.roll_num)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.s_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.marks)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.c_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fees)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.cell)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.user_id)
        </td>

        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.roll_num }) |
            @Html.ActionLink("Details", "Details", new { id = item.roll_num }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.roll_num })
        </td>
    </tr>
}

</table>
